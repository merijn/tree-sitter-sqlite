{
  "name": "tree-sitter-sqlite",
  "version": "0.1.0",
  "type": "module",
  "description": "SQLite grammar for tree-sitter",
  "repository": "https://github.com/merijn/tree-sitter-sqlite",
  "license": "GPL 3.0",
  "author": {
    "name": "Merijn Verstraaten",
    "email": "merijn@inconsistent.nl"
  },
  "main": "bindings/node",
  "types": "bindings/node",
  "keywords": [
    "incremental",
    "parsing",
    "tree-sitter",
    "sqlite"
  ],
  "files": [
    "grammar.js",
    "tree-sitter.json",
    "binding.gyp",
    "prebuilds/**",
    "bindings/node/*",
    "queries/*",
    "src/**",
    "*.wasm"
  ],
  "dependencies": {
    "node-addon-api": "^8.2.1",
    "node-gyp-build": "^4.8.2"
  },
  "devDependencies": {
    "prebuildify": "^6.0.1",
    "typescript": "^5.0.0",
    "tree-sitter-cli": "^0.25.3"
  },
  "peerDependencies": {
    "tree-sitter": "^0.21.1"
  },
  "peerDependenciesMeta": {
    "tree-sitter": {
      "optional": true
    }
  },
  "scripts": {
    "build": "tsc",
    "generate": "npm run build && tree-sitter generate dist/grammar.js",
    "install": "node-gyp-build",
    "prestart": "tree-sitter build --wasm",
    "start": "tree-sitter playground",
    "test": "node --test bindings/node/*_test.js"
  }
}
